{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "eol-last": 0,
    "no-console": 0,
    "new-cap": 0,
    "semi": 2,
    "jsx-quotes": 1,
    "strict": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
  }
}